{
	,"host": "https://translate.google.cn"
	,"index": ""
	,"style": "header,.VjFXz{display:none!important}"
	,"search": "/?sl=auto&tl=zh-CN&op=translate&text="
	,"searchx": [
		{keyx:"^[\u4e00-\u9fa5]", value:"/?sl=auto&tl=en&op=translate&text="}
	]
	,"keyPattern": "*"
	,"js": "let e = document.getElementById('gtyexp'),win=window,sel=win.getSelection()
	,ta = document.getElementsByTagName('textarea')[0];
	if(!e) {
		e=document.createElement('IMG');
		e.id='gtyexp';
		document.body.appendChild(e);
		e.src='//ssl.gstatic.com/translate/favicon.ico'; // 谷歌翻译的图标
		e.style='display:none;position:fixed;width:35px;height:35px;top:8px;right:25px;background-color:rgb(181 217 255);transition:all 0.15s;opacity:0.75;z-index:999;border-radius:3px;padding:10px;user-select:none;box-shadow:rgb(0 0 255 / 20%) 0px 7px 19px 0px;';
		function toggle(e){ 
			if(!sel.toStringRaw) { // 强硬作风 温和修改
				sel.toStringRaw = sel.toString;
				sel.toString = function(){
					var ret = sel.toStringRaw();
					if(win._PDRNG) {
						console.log('fatal sustain the selection v3='+win._PDRNG); 
						ret = win._PDRNG.toString();
						sel.removeAllRanges();
						sel.addRange(win._PDRNG);
						win._PDRNG = 0;
					}
					return ret
				}
			}
			ta.dispatchEvent(new KeyboardEvent('keydown', {
				keyCode: 0x10,
				bubbles: 1
			}));
			var rng=sel.getRangeAt(0),key=rng.toString();
			if(sel.anchorNode!=ta) 
			{
				win._PDRNG=rng.cloneRange();
				app.knock();
				win._PDRNG=0;
			}
			ta.dispatchEvent(new KeyboardEvent('keyup', {
				keyCode: 0x10,
				bubbles: 1
			}));
			//if(document.getElementsByClassName('aia4Ud')[0].firstElementChild.innerText==key) 
			{
				// 
				e.style.transform='scale(1.5)';
				console.log('找到了分词'+e.style.transform);
				//setTimeout(v=>{e.style.transform='unset'}, 150);
			}
		}
		e.onclick=toggle;
	}
	function sc(v){
		if(sel.type!='Range') e.style.display = 'none';
		else if(e.style.display=='none'){
			e.style.display = 'unset';
			e.style.opacity = 0.75;
		};
	};
	document.addEventListener('selectionchange', sc);
	if(ta) {
		ta.addEventListener('selectionchange', sc);
		ta.readOnly=true;
		
	}
	
	// 点击不显示候选列表
	function queryByAttr(aAttribute, aValue, abody) {
		return SearchElement(abody||document.body);
		function ElementVerifier(ele) {
			var Element = ele;
			if (Element.nodeName!=='#text' && ele.getAttribute(aAttribute)===aValue)
				//return eval('Element.' + aAttribute + '==\\'' + aValue + '\\'?true:false;');
				return ele;
			return 0;
		}
		function SearchElement(ele) {
			if (!ele) return 0;
			return ElementVerifier(ele) 
				|| SearchElement(ele.firstChild) 
				|| SearchElement(ele.nextSibling);
		}
	}
	let listPane;
	let textPara;
	function tweakHXCL(){ // 调整候选表
		var jn='jsname',cp=queryByAttr(jn, 'r5xl4'); // 解释内容容器
		function Qa(jsname, p){return queryByAttr(jn, jsname, p||cp)}
		if(!win._modin) {
			let modin = Qa('rhPddf').cloneNode(true); // 拷贝编辑按钮
			modin.style.color='#888';
			document.getElementsByClassName('FFpbKc')[0].appendChild(modin); // 添加到输入内容，字数栏之后
			modin.onclick=v=>{
				ta.readOnly=!ta.readOnly;
				modin.style.color=ta.readOnly?'#888':'#000';
				v.stopPropagation();
			}
			win._modin=modin;
		}
		//console.log('点击不显示候选列表', listPane = Qa('HyaQwf'));
		const observer=new MutationObserver(function(mutationsList, observer) {
			for(var mutation of mutationsList) {
				if (mutation.type==='attributes' && mutation.attributeName==='style') {
					//console.log('The ' + mutation.attributeName + ' attribute was modified.');
					if(listPane.style.display=='') { // 若正常显示候选列表
						listPane.style.display='none'; // 重新隐藏候选列表
						observer.disconnect();
					}
				}
			}
		});
		function obz(){
			observer.observe(listPane, { attributes: true
				, attributeFilter : ['style'] });
		}
		if(!win._PRGF)
		// mod the share btn
		win.addEventListener('click', v=>{
			var p=v.srcElement.parentElement;
			if(p) {
				if(p.getAttribute('jsname')==='ymIaV' && !v.srcElement.onclick) {
					v.srcElement.onclick=u=>{
						//console.log('fatal 123');
						if(!listPane) {
							listPane = Qa('HyaQwf');
							listPane.style.display='none'; //  hide the list pane
						}
						(textPara||Qa('W297wb')).click();  // clk on txt to show list pane
						listPane.style.display='';
						u.preventDefault();
						u.stopPropagation();
						obz();
						return false;
					};
				}
			}
			//console.log('fatal 123asd '+p.parentElement.className+' jn='+p.parentElement.getAttribute('jsname'));
			if(p.parentElement.className==='VIiyi') {
				textPara = v.srcElement;
				listPane = Qa('HyaQwf', p); // force hide the list pane
				if(listPane.style.display!='none')
					listPane.style.display='none';
			}
		}, true);
		win._tweakedHXCL=true;
		//console.log('点击不显示候选列表 done.');
	}
	win._tweakHXCL = tweakHXCL;
	if(!win._tweakedHXCL)
		setTimeout(tweakHXCL, 1350);
	//
	
	
	
	"
	,"searchJs": "var succ = 0;
try {
	var ta,tl=1,i=0;
	if(window._pdta){
		ta = window._pdta;
	}else{
		var tat = document.getElementsByTagName('textarea');
		for (ta=tat[0],tl=tat.length; i < tl; i++) if (tat[i].getAttribute('aria-label') === '原文') {
			ta=tat[i];
			window._pdta=ta;
			break;
		}
	}
	//console.log('fatal ta='+ta+' len='+tl+' wta='+window._pdta);
	if (ta) {
		ta.value = '%s';
		ta.dispatchEvent(new Event('input', {
			bubbles: true
		}));
		setTimeout(()=> {
			ta.dispatchEvent(new KeyboardEvent('keydown', {
				keyCode: 27,
				bubbles: 1
			}))
		}, 200);
		succ = i < tl;
		//window._tweakedHXCL = false; setTimeout(window._tweakHXCL, 350);
	}
} catch (e) {
	console.log('fatal e='+e);
}
succ"
}