{
	"host": "https://wantwords.thunlp.org"
	,"index": ""
	,"style": "#etyexp{position:fixed;width:35px;height:38px;top:8px;right:5px;opacity:0.45;transition:all 0.25s linear 0s;z-index:999999;padding:9px;user-select:none}"
	,"name": "万词王（智慧近义词查询）"
	,"js": "
		var win=window,doc=document;
		function debug(e){console.log('fatal web::'+e)};
		debug('init wantwords...');
		function ge(e){return doc.getElementById(e)};
		function initHBtn() {
			var e = doc.createElement('IMG');
			e.id='etyexp';
			if(!ge(e.id)) {
				doc.body.appendChild(e);
				e.src='/static/image/about.png';
				function toggle(e){var stl = ge('_PDSTL');
				e=e.srcElement;
				var mod = '.navbar{display: none;}#tabs{top:0!important}',idx=stl.innerText.indexOf(mod);
				if(idx>=0) {
					stl.innerText = stl.innerText.replace(mod, '');
					e.style.opacity=1;
				} else {
					stl.innerText = stl.innerText + mod;
					e.style.opacity=0.45;
				}}
				e.onclick=toggle;
			}
			e.click();
			win.headerBtn=e;
		}
		win.gotoid=function(){};
		if(!win._NWP) {
			win._NWP=[ge('result'),ge('result_EE'),ge('result_EC'),ge('result_CE')];
		}
		debug('init wantwords...');
	"
	,"searchJs": "
		if(!window.headerBtn && window.initHBtn) initHBtn();
		var succ;
		// 1汉语 3En 2汉英 4En-Zh
		// sid=0 2 1 3
		var rm=[1,2,3,4]
		var tid=rm[getSelectedTabId||0];
		var tab=$('#tabs-'+tid)[0];
		//debug('wants::',tid,tab,tab.id);
		// todo 自动切换
		if(tab) {
			tab.noword=1;
			var st=0;
			while(c=tab.children[st]) {
				if(c.id && c.id.startsWith('anchor')) st=1;
				else if(c.tagName!=='DIV') c.remove();
				else break;
			}
			var ta = tab.getElementsByTagName('INPUT')[0];
			ta.value='%s';
			tab.getElementsByTagName('BUTTON')[0].click();
			succ=1;
		}
		succ
"
	,"schVar": "
		// 搜索引擎变体
		function doSearch(e)
		{
			//debug('doSearch...');
			var id='ui-id-'+e,lnks=document.getElementsByTagName('A');
			for(var i=0;c=lnks[i++];) {
				if(c.id===id)
				{
					c.click();
					return 2;
				}
			}
		}
		doSearch
	"
	,"entrance": "js:@{schVar}(1)\r< 汉语 >
js:@{schVar}(2)\r< 汉英 >
js:@{schVar}(3)\r< English >
js:@{schVar}(4)\r< En-Zh >
	"
	,"onload": ""
	,"drawHighlightOnTop":"true"
	,"kikLetVar":"home.js"
}