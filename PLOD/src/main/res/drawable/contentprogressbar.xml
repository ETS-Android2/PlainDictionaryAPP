<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background" >
		<shape
			android:shape="rectangle">
			<corners android:radius="0dp"/>
			<solid
				android:color="#fff"
				/>
		</shape>
	</item>

	<item
		android:id="@android:id/progress"
		>
		<clip>
			<shape
				android:shape="rectangle">
				<corners android:radius="0dp"/>
				<solid
					android:color="#fff"
					/>
			</shape>
		</clip>
	</item>


</layer-list>